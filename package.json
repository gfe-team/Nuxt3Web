{
  "name": "nuxt3-web",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "node": ">= 14.10.0"
  },
  "scripts": {
    "dev": "nuxt dev  --dotenv env/.env.development",
    "build": "nuxt build --mode dev",
    "build:dev": "nuxt build --mode dev",
    "build:test": "nuxt build --mode test",
    "build:prod": "nuxt build --mode prod",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .vue --ext .js --ext .ts src/ --fix",
    "style": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "prettier:comment": "Automatically formatting all files in the current directory",
    "prettier": "prettier .  --write",
    "cz:comment": "Unified commit way: yarn cz",
    "cz": "git add . && cz",
    "tag": "npm version patch && git push && git push origin --tags",
    "start": "npm run dev"
  },
  "dependencies": {
    "@awesome-image/image": "^0.0.10",
    "@awesome-image/services": "^0.0.2",
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/nuxt": "^9.6.0",
    "ant-design-vue": "^3.2.15",
    "pinia": "^2.0.28",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@intlify/nuxt3": "^0.2.3",
    "@nuxt/content": "^2.0.1",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "auto-import-types": "^0.0.4",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.2.6",
    "commitlint": "^17.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "nuxt": "3.0.0",
    "prettier": "^2.8.1",
    "sass": "1.53.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.21.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.20.3"
  }
}
